        <success>Always</success>
        <xpath>Defs/ThingDef[defName="VD_TerrestrialGlobe"]/designationCategory</xpath>
        <value>
            <designationCategory>Ferny_Auxiliaries</designationCategory>
        </value>
    </Operation>
</Patch>
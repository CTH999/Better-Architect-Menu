<?xml version="1.0" encoding="utf-8" ?>          
<Patch>       
                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="CashRegister_CashRegister"]/designationCategory</xpath>
                    <value>
                        <designationCategory>Ferny_Market</designationCategory>
                    </value>
                </Operation>

              <Operation Class="PatchOperationFindMod">
                <mods>
                  <li>Progression: Hospitality</li>
                </mods>
                <match Class="PatchOperationSequence">
                  <operations>
                    <li Class="PatchOperationReplace">
                      <success>Always</success>
                      <xpath>Defs/ThingDef[defName="Ferny_MerchantBox"]/designationCategory</xpath>
                      <value>
                        <designationCategory>Ferny_Market</designationCategory>
                      </value>
                    </li>
                  </operations>
                </match>
              </Operation>

</Patch>